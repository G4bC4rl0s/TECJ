RewriteEngine On

# Permitir acesso direto a arquivos CSS, JS e imagens
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^(css|js|images|img)/.*$ - [L]

# Redirecionar tudo para index.php se n√£o for um arquivo real
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php [QSA,L]